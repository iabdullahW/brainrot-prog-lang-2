{
  "name": "brainrot",
  "displayName": "brainrot",
  "description": "Enables syntax highlighting for Brainrot programming language.",
  "author": "Leonardo Araujo",
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/araujo88/brainrot-vscode-support"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "brainrot",
        "aliases": [
          "Brainrot",
          "brainrot"
        ],
        "extensions": [
          ".brainrot"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "brainrot",
        "scopeName": "source.brainrot",
        "path": "./syntaxes/brainrot.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  }
}
